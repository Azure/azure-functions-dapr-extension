{
  "bindings": [
    {
      "type": "httpTrigger",
      "route": "state/{key}",
      "methods": [
        "post"
      ],
      "authLevel": "function",
      "name": "req"
    },
    {
      "type": "daprState",
      "direction": "out",
      "name": "state",
      "stateStore": "statestore",
      "key": "{key}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ]
}